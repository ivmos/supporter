{
  "name": "Supporter",
  "version": "1.0.0",
  "description": "A customer support live chat",
  "author": "colymore",
  "main": "index.js",
  "engines": {
    "node": ">=4.8.0",
    "npm": ">=2.15.11",
    "yarn": ">=0.20.3"
  },
  "scripts": {
    "start": "gulp serve",
    "start:debug": "cross-env DEBUG=api:* yarn start",
    "build": "gulp",
    "lint": "esw *.js server config --color",
    "lint:watch": "yarn lint -- --watch",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors --compilers js:babel-core/register server/tests --recursive",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors --compilers js:babel-core/register server/tests --recursive",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:colymore/supporter.git"
  },
  "dependencies": {
    "bluebird": "3.4.6",
    "body-parser": "1.15.2",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.1",
    "debug": "2.6.8",
    "dotenv": "4.0.0",
    "express": "4.14.0",
    "express-jwt": "5.3.0",
    "express-validation": "1.0.1",
    "express-unless": "0.3.0",
    "express-ws":"3.0.0",
    "express-winston": "2.1.2",
    "helmet": "3.1.0",
    "http-status": "1.0.1",
    "joi": "10.0.6",
    "jsonwebtoken": "7.4.1",
    "method-override": "2.3.9",
    "mongoose": "4.10.8",
    "morgan": "1.7.0",
    "promised-bcrypt": "0.2.2",
    "redis": "0.12.1",
    "winston": "2.3.0"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "chai": "^3.4.1",
    "commitizen": "^2.9.2",
    "coveralls": "^2.11.6",
    "cross-env": "3.1.3",
    "cz-conventional-changelog": "1.2.0",
    "del": "^2.2.0",
    "eslint": "3.16.1",
    "eslint-config-airbnb-base": "7.1.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-watch": "2.1.14",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-load-plugins": "^1.2.0",
    "gulp-newer": "^1.1.0",
    "gulp-nodemon": "^2.0.6",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "3.2.0",
    "run-sequence": "^1.1.5",
    "supertest": "2.0.1",
    "supertest-as-promised": "4.0.2"
  },
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}